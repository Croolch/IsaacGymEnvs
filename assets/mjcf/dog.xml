        <geom name="waist" type="sphere" pos="0.17 0 -0.025" size="0.1" mass="5"/>
        <!-- <geom name="joint_spine0" type="sphere" pos="0.075 0 -0.02" size="0.05" mass="2"/> -->
        <site name="waist" class="touch" type="sphere" pos="0.17 0 -0.025" size="0.11"/>
