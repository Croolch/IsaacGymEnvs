            <joint name="left_shoulder_y" pos="0 0 0" axis="0 1 0" range="-60 90" stiffness="500" damping="50" armature=".02"/>
            <joint name="left_shoulder_z" pos="0 0 0" axis="0 0 1" range="-50 50" stiffness="500" damping="50" armature=".02"/>
